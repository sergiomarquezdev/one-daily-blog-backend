{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                },
                "modules": "commonjs"
            }
        ],
        [
            "@babel/preset-typescript",
            {
                "rewriteImportExtensions": true
            }
        ]
    ],
    "plugins": ["@babel/plugin-transform-typescript"]
}
